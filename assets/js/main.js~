require.config({
	baseUrl: 'bower_components',
	waitSeconds: 0,
  	paths: {
    	angular : 'angular/angular.min',
	angularjsui : 'angularjs/src/js/angular-ui-bootstrap.js',
    	underscore : "underscore/underscore",
	requirejs-domready : "requirejs-domready/domReady"
  	},
  	shim: {
    	'angular' : {
      		exports : 'angular'
    	},
    	'underscore' : {
      		exports : "_"
    	},
    	'angularjsui' : {
      		exports: "angularjsui"
    	},
	'requirejs-domready' : {
		exports: "requiredomready"
	}
  	}
});

var setupApp = function () {
  	require(["app",'angular', '_'], function (app,angular,_) {
    	app.advanceReadiness(); 
    });
};
window.addEventListener("load", setupApp(), false);
